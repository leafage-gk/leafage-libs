{
  "name": "@leafage-libs/header",
  "version": "1.0.0",
  "description": "corporate header component",
  "main": "dist/leafage-header.umd.js",
  "module": "dist/leafage-header.esm.js",
  "unpkg": "dist/leafage-header.min.js",
  "browser": {
    "./sfc": "./src/LeafageHeader.vue"
  },
  "repository": "https://github.com/leafage-gk/corporate-libs",
  "author": "tnkgs <kento@kt-one.jp>",
  "license": "UNLICENSED",
  "scripts": {
    "build": "yarn run build:umd & yarn run build:es & yarn run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/leafage-header.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/leafage-header.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/leafage-header.min.js"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  }
}
