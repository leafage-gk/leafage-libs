{
  "name": "@leafage-gk/lp-utils",
  "version": "0.0.0",
  "description": "landing page library",
  "main": "dist/lp-utils.umd.js",
  "module": "dist/lp-utils.esm.js",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/leafage-gk/leafage-libs.git",
    "directory": "packages/lp-utils"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "tnkgs <kento@kt-one.jp>",
  "license": "UNLICENSED",
  "scripts": {
    "prepublish": "yarn build",
    "build": "yarn build:css & yarn build:umd & yarn build:es",
    "build:css": "node-sass --output-style compressed src/scss/base.scss dist/base.css",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/lp-utils.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/lp-utils.esm.js"
  },
  "devDependencies": {
    "node-sass": "^4.14.1"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vue-head": "^2.2.0"
  }
}
